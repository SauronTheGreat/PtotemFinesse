<h1>Listing survey_positions</h1>

<table>
  <tr>
    <th>Position</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @survey_positions.each do |survey_position| %>
  <tr>
    <td><%= survey_position.position %></td>
    <td><%= link_to 'Show', survey_position %></td>
    <td><%= link_to 'Edit', edit_survey_position_path(survey_position) %></td>
