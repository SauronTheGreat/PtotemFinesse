<h1>Listing case_studies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Max round</th>
    <th>Student_group_case_study_id</th>
    <th></th>
    <th></th>
  </tr>

<% @case_studies.each do |case_study| %>
  <tr>
    <td><%= case_study.name %></td>
    <td><%= case_study.max_round %></td>
    <td><%= link_to 'Show', case_study %></td>
    <td><%= link_to 'Edit', edit_case_study_path(case_study) %></td>
    <td><%= link_to 'Destroy', case_study, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Case study', new_case_study_path %>
