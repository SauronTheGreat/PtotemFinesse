<h1>Listing survey_questions</h1>

<table>
  <tr>
    <th>Survey</th>
    <th>Question</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @survey_questions.each do |survey_question| %>
  <tr>
    <td><%= survey_question.survey_id %></td>
    <td><%= survey_question.question_id %></td>
    <td><%= link_to 'Show', survey_question %></td>
    <td><%= link_to 'Edit', edit_survey_question_path(survey_question) %></td>
    <td><%= link_to 'Destroy', survey_question, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

