<h1>Listing employee_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Cost</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @employee_types.each do |employee_type| %>
  <tr>
    <td><%= employee_type.name %></td>
    <td><%= employee_type.cost %></td>
    <td><%= link_to 'Show', employee_type %></td>
    <td><%= link_to 'Edit', edit_employee_type_path(employee_type) %></td>
    <td><%= link_to 'Destroy', employee_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

